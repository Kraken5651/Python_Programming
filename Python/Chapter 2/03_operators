# Arithematic Operators

a = 30

b = 10

c = (a+b)

print(c)

# Assignement Operator 

a = 4-1
print(a)

b = 9
b += 2 # Increment the value of b by 2
print(b)
b -= 3 # Decrement the value of b by 3
print(b)

# Comparison Operators

d = 5>7
print(d)

d = 5<7
print(d)

#Logical Operators

e = True or False
print(e)

f = True and True
print(f)

g = not(True)
print(g)